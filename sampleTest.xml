        <dataItem>
          <name>password</name>
          <value>--password--</value>
        </dataItem>
      </postData>
    </request>
    <request>
      <url>http://www.targeturl.com/pageToTest.aspx</url>
      <type>GET</type>
    </request>
  </requests>
</test>